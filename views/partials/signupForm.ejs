<form id="signupForm" enctype='application/x-www-form-urlencoded'>
    <label for="name">Full Name</label><br>
    <input type="text" name="name" id="name" placeholder="Your full name eg. John Doe" ><br>
    <label for="email">Email ID</label><br>
    <input type="email" name="email" id="email" placeholder="Your email id eg. johndoe28@gmail.com" ><br>
    <label for="dob">Date of birth</label><br>
    <input type="date" name="dob" id="dob" placeholder="dd-mm-yyyy" ><br>
    <label for="usrname">Choose a user Name</label><br>
    <input type="text" name="usrname" id="usrname" placeholder="eg. john_doe96" ><br>
    <label for="password">Choose a password</label><br>
    <input type="password" name="password" id="password" placeholder="Select a strong password" ><br>
    <button type="submit">Submit</button>
</form><br>
<h2 id="status"></h2>
<button id="go" style="visibility: hidden; background-color: rbg(252, 211, 73); border-radius: 5px; color: black; font-family: 'Quicksand', sans-serif;"><a href="http://localhost:8080/app/home">Let's go</a></button>
<p id="info"></p>